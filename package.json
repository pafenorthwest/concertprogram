		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.7",
		"@types/jsdom": "^21.1.7",
		"@types/papaparse": "^5.3.15",
		"@types/pg": "^8.11.6",
		"dompurify": "^3.2.1",
		"dotenv": "^17.2.1",
		"eslint": "^9.0.0",
